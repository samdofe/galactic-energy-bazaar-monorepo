{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "typeRoots": ["./", "node_modules/@types"],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@feds/cdk-side-nav": [
        "libs/feds-cdk-libs/feds-cdk-side-nav/src/index.ts"
      ],
      "@feds/cdk/*": ["libs/feds-cdk/src/*"],
      "@feds/cdk/side-nav": ["libs/feds-cdk/side-nav/src/index.ts"],
      "@feds/core-auth": ["libs/feds-core-libs/feds-core-auth/src/index.ts"],
      "@feds/core/*": ["libs/feds-core/src/*"],
      "@feds/core/auth": ["libs/feds-core/auth/src/index.ts"],
      "@feds/styles": ["libs/feds-styles/src/index.ts"],
      "dashboard/Routes": [
        "apps/dashboard/src/app/remote-entry/entry.routes.ts"
      ],
      "login/Routes": ["apps/login/src/app/remote-entry/entry.routes.ts"],
      "trades/Routes": ["apps/trades/src/app/remote-entry/entry.routes.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
